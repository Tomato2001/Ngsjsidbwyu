-- –°–æ–∑–¥–∞–µ–º –ª–æ–∫–∞–ª—å–Ω—É—é –ø–µ—Ä–µ–º–µ–Ω–Ω—É—é –¥–ª—è —Ö—Ä–∞–Ω–µ–Ω–∏—è –∏–≥—Ä–æ–∫–∞
local player = game.Players.LocalPlayer

-- –°–æ–∑–¥–∞–µ–º –ª–æ–∫–∞–ª—å–Ω—É—é –ø–µ—Ä–µ–º–µ–Ω–Ω—É—é –¥–ª—è —Ö—Ä–∞–Ω–µ–Ω–∏—è –≥—Ä–∞—Ñ–∏—á–µ—Å–∫–æ–≥–æ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–∞
local gui = Instance.new("ScreenGui", player.PlayerGui)

-- –°–æ–∑–¥–∞–µ–º –ª–æ–∫–∞–ª—å–Ω—É—é –ø–µ—Ä–µ–º–µ–Ω–Ω—É—é –¥–ª—è —Ö—Ä–∞–Ω–µ–Ω–∏—è —Ñ—Ä–µ–π–º–∞ –º–µ–Ω—é
local menu = Instance.new("Frame", gui)
menu.Name = "Menu"
menu.Size = UDim2.new(0.7, 0, 0.8, 0)
menu.Position = UDim2.new(0.15, 0, 0.08, 0)
menu.BackgroundTransparency = 1
menu.Draggable = true -- –í–∫–ª—é—á–∞–µ–º —Ñ—É–Ω–∫—Ü–∏—é –ø–µ—Ä–µ—Ç–∞—Å–∫–∏–≤–∞–Ω–∏—è

-- –ü–æ—Å–ª–µ —Å–æ–∑–¥–∞–Ω–∏—è –æ–±—ä–µ–∫—Ç–∞ 'menu', –Ω–æ –ø–µ—Ä–µ–¥ —Å–æ–∑–¥–∞–Ω–∏–µ–º –∫–Ω–æ–ø–æ–∫:
local backgroundImage = Instance.new("ImageLabel")
backgroundImage.Name = "BackgroundImage"
backgroundImage.Size = UDim2.new(1, 0, 1, 0) -- –ó–∞–Ω–∏–º–∞–µ—Ç –≤–µ—Å—å —Ñ—Ä–µ–π–º –º–µ–Ω—é
backgroundImage.Position = UDim2.new(0, 0, 0, 0)
backgroundImage.Image = "rbxassetid://16218823113" -- –ó–∞–º–µ–Ω–∏—Ç–µ –≤–∞—à_asset_id –Ω–∞ –∞–∫—Ç—É–∞–ª—å–Ω—ã–π AssetId –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏—è
backgroundImage.ScaleType = Enum.ScaleType.Stretch -- –†–∞—Å—Ç—è–≥–∏–≤–∞–µ–º –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏–µ –Ω–∞ –≤–µ—Å—å —Ñ—Ä–µ–π–º
backgroundImage.ZIndex = 1 -- –£—Å—Ç–∞–Ω–æ–≤–∏—Ç–µ ZIndex –º–µ–Ω—å—à–µ, —á–µ–º —É –∫–Ω–æ–ø–æ–∫
backgroundImage.Parent = menu -- –†–æ–¥–∏—Ç–µ–ª—å—Å–∫–∏–π —ç–ª–µ–º–µ–Ω—Ç - —ç—Ç–æ —Ñ—Ä–µ–π–º –º–µ–Ω—é

-- –î–µ–ª–∞–µ–º —Ñ–æ–Ω —Ñ—Ä–µ–π–º–∞ –º–µ–Ω—é –ø–æ–ª–Ω–æ—Å—Ç—å—é –ø—Ä–æ–∑—Ä–∞—á–Ω—ã–º
menu.BackgroundTransparency = 1


-- –°–æ–∑–¥–∞–µ–º –ª–æ–∫–∞–ª—å–Ω—É—é –ø–µ—Ä–µ–º–µ–Ω–Ω—É—é –¥–ª—è —Ö—Ä–∞–Ω–µ–Ω–∏—è –∑–∞–≥–æ–ª–æ–≤–∫–∞ –º–µ–Ω—é
local title = Instance.new("TextLabel", menu)
title.Name = "Title"
title.Size = UDim2.new(1, 0, 0.1, 0)
title.Position = UDim2.new(0, 0, 0, 0)
title.BackgroundColor3 = Color3.new(0, 0, 0)
title.BackgroundTransparency = 0.5
title.Text = "üçÖ Tomato Hub üçÖ"
title.TextColor3 = Color3.new(1, 1, 1)
title.TextScaled = true
title.Font = Enum.Font.SourceSansBold

-- –°–æ–∑–¥–∞–µ–º –ª–æ–∫–∞–ª—å–Ω—É—é –ø–µ—Ä–µ–º–µ–Ω–Ω—É—é –¥–ª—è —Ö—Ä–∞–Ω–µ–Ω–∏—è –∫–Ω–æ–ø–∫–∏ –∑–∞–∫—Ä—ã—Ç–∏—è –º–µ–Ω—é
local closeButton = Instance.new("TextButton", menu)
closeButton.Name = "CloseButton"
closeButton.Size = UDim2.new(0.1, 0, 0.1, 0)
closeButton.Position = UDim2.new(0.9, 0, 0, 0)
closeButton.BackgroundColor3 = Color3.new(1, 0, 0)
closeButton.BackgroundTransparency = 0.5
closeButton.Text = "–•"
closeButton.TextColor3 = Color3.new(1, 1, 1)
closeButton.TextScaled = true
closeButton.Font = Enum.Font.SourceSansBold

-- –ü—Ä–µ–¥–ø–æ–ª–æ–∂–∏–º, —á—Ç–æ closeButton ‚Äî —ç—Ç–æ –∫–Ω–æ–ø–∫–∞ –∑–∞–∫—Ä—ã—Ç–∏—è –Ω–∞ –ø–µ—Ä–≤–æ–π —Å—Ç—Ä–∞–Ω–∏—Ü–µ
closeButton.MouseButton1Click:Connect(function()
    menu.Visible = false -- –°–∫—Ä—ã–≤–∞–µ–º —Ç–æ–ª—å–∫–æ –ø–µ—Ä–≤—É—é —Å—Ç—Ä–∞–Ω–∏—Ü—É
end)


-- –°–æ–∑–¥–∞–µ–º –ª–æ–∫–∞–ª—å–Ω—É—é –ø–µ—Ä–µ–º–µ–Ω–Ω—É—é –¥–ª—è —Ö—Ä–∞–Ω–µ–Ω–∏—è –∫–Ω–æ–ø–∫–∏ —Å–≤–æ—Ä–∞—á–∏–≤–∞–Ω–∏—è –º–µ–Ω—é
local minimizeButton = Instance.new("TextButton", menu)
minimizeButton.Name = "MinimizeButton"
minimizeButton.Size = UDim2.new(0.1, 0, 0.1, 0)
minimizeButton.Position = UDim2.new(0.8, 0, 0, 0) -- –°–¥–≤–∏–≥–∞–µ–º –Ω–∞–ª–µ–≤–æ –æ—Ç–Ω–æ—Å–∏—Ç–µ–ª—å–Ω–æ –∫–Ω–æ–ø–∫–∏ –∑–∞–∫—Ä—ã—Ç–∏—è
minimizeButton.BackgroundColor3 = Color3.new(0, 0, 1)
minimizeButton.BackgroundTransparency = 0.5
minimizeButton.Text = "‚Äì"
minimizeButton.TextColor3 = Color3.new(1, 1, 1)
minimizeButton.TextScaled = true
minimizeButton.Font = Enum.Font.SourceSansBold

-- –°–æ–∑–¥–∞–µ–º –ª–æ–∫–∞–ª—å–Ω—É—é –ø–µ—Ä–µ–º–µ–Ω–Ω—É—é –¥–ª—è —Ö—Ä–∞–Ω–µ–Ω–∏—è –∫–Ω–æ–ø–∫–∏ —Ä–∞–∑–≤–æ—Ä–∞—á–∏–≤–∞–Ω–∏—è –º–µ–Ω—é
local expandButton = Instance.new("TextButton", gui)
expandButton.Name = "ExpandButton"
expandButton.Size = UDim2.new(0.1, 0, 0.1, 0)
expandButton.Position = UDim2.new(0.1, 0, 0.1, 0) -- –ü–æ–∑–∏—Ü–∏—è –Ω–∞ —ç–∫—Ä–∞–Ω–µ
expandButton.BackgroundColor3 = Color3.new(0, 0, 0)
expandButton.BackgroundTransparency = 0.5
expandButton.Text = "+"
expandButton.TextColor3 = Color3.new(1, 1, 1)
expandButton.TextScaled = true
expandButton.Font = Enum.Font.SourceSansBold
expandButton.Visible = false -- –°–Ω–∞—á–∞–ª–∞ –∫–Ω–æ–ø–∫–∞ –Ω–µ–≤–∏–¥–∏–º–∞

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–≤–æ—Ä–∞—á–∏–≤–∞–Ω–∏—è –º–µ–Ω—é
minimizeButton.MouseButton1Click:Connect(function()
    menu.Visible = false
    expandButton.Visible = true
end)

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Ä–∞–∑–≤–æ—Ä–∞—á–∏–≤–∞–Ω–∏—è –º–µ–Ω—é
expandButton.MouseButton1Click:Connect(function()
    menu.Visible = true
    expandButton.Visible = false
end)

-- –ü—Ä–µ–¥–ø–æ–ª–∞–≥–∞–µ–º, —á—Ç–æ –≤–µ—Å—å –ø—Ä–µ–¥—ã–¥—É—â–∏–π –∫–æ–¥ –æ—Å—Ç–∞–µ—Ç—Å—è –±–µ–∑ –∏–∑–º–µ–Ω–µ–Ω–∏–π

-- –°–æ–∑–¥–∞–µ–º –≤—Ç–æ—Ä—É—é —Å—Ç—Ä–∞–Ω–∏—Ü—É –º–µ–Ω—é
local secondPage = Instance.new("Frame", gui)
secondPage.Name = "SecondPage"
secondPage.Size = UDim2.new(0.7, 0, 0.8, 0)
secondPage.Position = UDim2.new(0.15, 0, 0.08, 0)
secondPage.BackgroundTransparency = 1
secondPage.Visible = false -- –ò–∑–Ω–∞—á–∞–ª—å–Ω–æ —Å–∫—Ä—ã—Ç–∞

-- –î–æ–±–∞–≤–ª—è–µ–º —Ñ–æ–Ω, –∞–Ω–∞–ª–æ–≥–∏—á–Ω—ã–π –ø–µ—Ä–≤–æ–π —Å—Ç—Ä–∞–Ω–∏—Ü–µ
local secondPageBackground = backgroundImage:Clone()
secondPageBackground.Parent = secondPage

-- –°–æ–∑–¥–∞–µ–º –∫–Ω–æ–ø–∫—É –¥–ª—è –ø–µ—Ä–µ—Ö–æ–¥–∞ –Ω–∞ –≤—Ç–æ—Ä—É—é —Å—Ç—Ä–∞–Ω–∏—Ü—É
local nextPageButton = Instance.new("TextButton", menu)
nextPageButton.Name = "NextPageButton"
nextPageButton.Size = UDim2.new(0.1, 0, 0.1, 0)
nextPageButton.Position = UDim2.new(0, 0, 0.9, 0) -- –í –ª–µ–≤–æ–º –Ω–∏–∂–Ω–µ–º —É–≥–ª—É
nextPageButton.BackgroundColor3 = Color3.new(0, 0, 0)
nextPageButton.Text = ">"
nextPageButton.TextColor3 = Color3.new(1, 1, 1)
nextPageButton.Font = Enum.Font.SourceSansBold
nextPageButton.TextScaled = true

-- –°–æ–∑–¥–∞–µ–º –º–∞–ª–µ–Ω—å–∫–æ–µ –º–µ–Ω—é –¥–ª—è –≤–≤–æ–¥–∞ –∫–æ–¥–∞
local codeInputMenu = Instance.new("Frame", gui)
codeInputMenu.Name = "CodeInputMenu"
codeInputMenu.Size = UDim2.new(0.2, 0, 0.2, 0)
codeInputMenu.Position = UDim2.new(0.4, 0, 0.4, 0)
codeInputMenu.BackgroundColor3 = Color3.new(0, 0, 0)
codeInputMenu.Visible = false -- –ò–∑–Ω–∞—á–∞–ª—å–Ω–æ —Å–∫—Ä—ã—Ç–æ

-- closeButton –≤ —ç—Ç–æ–º –∫–æ–Ω—Ç–µ–∫—Å—Ç–µ ‚Äî —ç—Ç–æ –∫–Ω–æ–ø–∫–∞ –∑–∞–∫—Ä—ã—Ç–∏—è –≤ –º–µ–Ω—é –≤–≤–æ–¥–∞ –∫–æ–¥–∞
local closeButtonCodeMenu = Instance.new("TextButton", codeInputMenu) -- –£–±–µ–¥–∏—Ç–µ—Å—å, —á—Ç–æ —Å–æ–∑–¥–∞–µ—Ç–µ –∫–Ω–æ–ø–∫—É –≤ –ø—Ä–∞–≤–∏–ª—å–Ω–æ–º —Ä–æ–¥–∏—Ç–µ–ª—å—Å–∫–æ–º —ç–ª–µ–º–µ–Ω—Ç–µ
closeButtonCodeMenu.Size = UDim2.new(0.2, 0, 0.2, 0)
closeButtonCodeMenu.Position = UDim2.new(0.8, 0, 0, 0) -- –í –ø—Ä–∞–≤–æ–º –≤–µ—Ä—Ö–Ω–µ–º —É–≥–ª—É –º–∞–ª–µ–Ω—å–∫–æ–≥–æ –º–µ–Ω—é
closeButtonCodeMenu.BackgroundColor3 = Color3.new(1, 0, 0)
closeButtonCodeMenu.Text = "X"
closeButtonCodeMenu.TextColor3 = Color3.new(1, 1, 1)
closeButtonCodeMenu.Font = Enum.Font.SourceSansBold
closeButtonCodeMenu.TextScaled = true

-- –û–±—Ä–∞–±–æ—Ç—á–∏–∫ –Ω–∞–∂–∞—Ç–∏—è –Ω–∞ –∫—Ä–µ—Å—Ç–∏–∫ –º–∞–ª–µ–Ω—å–∫–æ–≥–æ –º–µ–Ω—é
closeButtonCodeMenu.MouseButton1Click:Connect(function()
    codeInputMenu.Visible = false -- –°–∫—Ä—ã–≤–∞–µ–º —Ç–æ–ª—å–∫–æ –º–∞–ª–µ–Ω—å–∫–æ–µ –º–µ–Ω—é –≤–≤–æ–¥–∞ –∫–æ–¥–∞
end)


-- –ü–æ–ª–µ –¥–ª—è –≤–≤–æ–¥–∞ –∫–æ–¥–∞
local codeInput = Instance.new("TextBox", codeInputMenu)
codeInput.Size = UDim2.new(0.8, 0, 0.6, 0)
codeInput.Position = UDim2.new(0.1, 0, 0.2, 0)
codeInput.BackgroundColor3 = Color3.new(1, 1, 1)
codeInput.Text = ""
codeInput.Font = Enum.Font.SourceSans
codeInput.TextScaled = true

-- –õ–æ–≥–∏–∫–∞ –æ—Ç–∫—Ä—ã—Ç–∏—è –∏ –∑–∞–∫—Ä—ã—Ç–∏—è –º–µ–Ω—é –∫–æ–¥–∞
nextPageButton.MouseButton1Click:Connect(function()
    codeInputMenu.Visible = true
end)

closeButton.MouseButton1Click:Connect(function()
    codeInputMenu.Visible = false
end)

-- –ü—Ä–æ–≤–µ—Ä–∫–∞ –∫–æ–¥–∞ –∏ –ø–µ—Ä–µ—Ö–æ–¥ –Ω–∞ –≤—Ç–æ—Ä—É—é —Å—Ç—Ä–∞–Ω–∏—Ü—É
codeInput.FocusLost:Connect(function(enterPressed)
    if enterPressed and codeInput.Text == "123" then
        menu.Visible = false
        secondPage.Visible = true
        codeInputMenu.Visible = false
    else
        codeInput.Text = "INCORRECT CODE"
    end
end)

-- –°–æ–∑–¥–∞–µ–º –æ–±—ä–µ–∫—Ç ImageLabel –¥–ª—è —Ñ–æ–Ω–∞ –≤—Ç–æ—Ä–æ–π —Å—Ç—Ä–∞–Ω–∏—Ü—ã —Å –¥—Ä—É–≥–∏–º –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏–µ–º
local secondPageBackgroundImage = Instance.new("ImageLabel")
secondPageBackgroundImage.Name = "BackgroundImage"
secondPageBackgroundImage.Size = UDim2.new(1, 0, 1, 0) -- –ó–∞–Ω–∏–º–∞–µ—Ç –≤–µ—Å—å —Ñ—Ä–µ–π–º –≤—Ç–æ—Ä–æ–π —Å—Ç—Ä–∞–Ω–∏—Ü—ã
secondPageBackgroundImage.Position = UDim2.new(0, 0, 0, 0)
secondPageBackgroundImage.Image = "rbxassetid://16252558153" -- –ó–∞–º–µ–Ω–∏—Ç–µ –Ω–∞ –Ω–æ–≤—ã–π AssetId –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏—è
secondPageBackgroundImage.ScaleType = Enum.ScaleType.Stretch -- –†–∞—Å—Ç—è–≥–∏–≤–∞–µ–º –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏–µ –Ω–∞ –≤–µ—Å—å —Ñ—Ä–µ–π–º
secondPageBackgroundImage.ZIndex = 1 -- –£—Å—Ç–∞–Ω–æ–≤–∏—Ç–µ ZIndex –º–µ–Ω—å—à–µ, —á–µ–º —É –∫–Ω–æ–ø–æ–∫
secondPageBackgroundImage.Parent = secondPage -- –†–æ–¥–∏—Ç–µ–ª—å—Å–∫–∏–π —ç–ª–µ–º–µ–Ω—Ç - —ç—Ç–æ —Ñ—Ä–µ–π–º –≤—Ç–æ—Ä–æ–π —Å—Ç—Ä–∞–Ω–∏—Ü—ã

-- –î–µ–ª–∞–µ–º —Ñ–æ–Ω —Ñ—Ä–µ–π–º–∞ –≤—Ç–æ—Ä–æ–π —Å—Ç—Ä–∞–Ω–∏—Ü—ã –ø–æ–ª–Ω–æ—Å—Ç—å—é –ø—Ä–æ–∑—Ä–∞—á–Ω—ã–º
secondPage.BackgroundTransparency = 1



-- –ö–Ω–æ–ø–∫–∞ –≤–æ–∑–≤—Ä–∞—Ç–∞ –Ω–∞ –ø–µ—Ä–≤—É—é —Å—Ç—Ä–∞–Ω–∏—Ü—É
local previousPageButton = Instance.new("TextButton", secondPage)
previousPageButton.Name = "PreviousPageButton"
previousPageButton.Size = UDim2.new(0.1, 0, 0.1, 0)
previousPageButton.Position = UDim2.new(0, 0, 0.9, 0) -- –í –ª–µ–≤–æ–º –Ω–∏–∂–Ω–µ–º —É–≥–ª—É
previousPageButton.BackgroundColor3 = Color3.new(0, 0, 0)
previousPageButton.Text = "<"
previousPageButton.TextColor3 = Color3.new(1, 1, 1)
previousPageButton.Font = Enum.Font.SourceSansBold
previousPageButton.TextScaled = true

previousPageButton.MouseButton1Click:Connect(function()
    secondPage.Visible = false
    menu.Visible = true
end)

-- –î–æ–±–∞–≤–ª—è–µ–º –æ–±—ä–µ–∫—Ç Sound –¥–ª—è –≤–æ—Å–ø—Ä–æ–∏–∑–≤–µ–¥–µ–Ω–∏—è –º—É–∑—ã–∫–∏
local music = Instance.new("Sound", secondPage)
music.SoundId = "rbxassetid://5410080857" -- –ó–∞–º–µ–Ω–∏—Ç–µ –Ω–∞ –≤–∞—à –∞–π–¥–∏ –º—É–∑—ã–∫–∏
music.Name = "BackgroundMusic"
music.Looped = true -- –ú—É–∑—ã–∫–∞ –±—É–¥–µ—Ç –∑–∞—Ü–∏–∫–ª–µ–Ω–∞

-- –°–æ–∑–¥–∞–µ–º –∫–Ω–æ–ø–∫—É –≤–æ—Å–ø—Ä–æ–∏–∑–≤–µ–¥–µ–Ω–∏—è/–æ—Å—Ç–∞–Ω–æ–≤–∫–∏ –º—É–∑—ã–∫–∏ –Ω–∞ –≤—Ç–æ—Ä–æ–π —Å—Ç—Ä–∞–Ω–∏—Ü–µ
local playButton = Instance.new("TextButton", secondPage)
playButton.Name = "PlayButton"
playButton.Size = UDim2.new(0.2, 0, 0.1, 0) -- –†–∞–∑–º–µ—Ä –∫–Ω–æ–ø–∫–∏
playButton.Position = UDim2.new(0.4, 0, 0.45, 0) -- –ü–æ–∑–∏—Ü–∏—è –∫–Ω–æ–ø–∫–∏ –ø–æ —Ü–µ–Ω—Ç—Ä—É —Å—Ç—Ä–∞–Ω–∏—Ü—ã
playButton.BackgroundColor3 = Color3.new(1, 0, 0) -- –ö—Ä–∞—Å–Ω—ã–π —Ñ–æ–Ω –∫–Ω–æ–ø–∫–∏
playButton.Text = "START ‚äô‚Å†Ôπè‚Å†‚äô" -- –ù–∞—á–∞–ª—å–Ω—ã–π —Ç–µ–∫—Å—Ç –∫–Ω–æ–ø–∫–∏
playButton.TextColor3 = Color3.new(1, 1, 1)
playButton.Font = Enum.Font.SourceSansBold
playButton.TextScaled = true

-- –°–æ–∑–¥–∞–µ–º —Ç–µ–∫—Å—Ç "FREE MP3! :3" —Å–≤–µ—Ä—Ö—É –∫–Ω–æ–ø–∫–∏
local freeMp3Text = Instance.new("TextLabel", secondPage)
freeMp3Text.Name = "FreeMp3Text"
freeMp3Text.Size = UDim2.new(0.2, 0, 0.09, 0) -- –†–∞–∑–º–µ—Ä —Ç–µ–∫—Å—Ç–∞
freeMp3Text.Position = UDim2.new(0.4, 0, 0.55, 0) -- –ü–æ–∑–∏—Ü–∏—è —Ç–µ–∫—Å—Ç–∞ –Ω–∞–¥ –∫–Ω–æ–ø–∫–æ–π
freeMp3Text.BackgroundColor3 = Color3.new(1, 1, 0) -- –ñ–µ–ª—Ç—ã–π —Ñ–æ–Ω —Ç–µ–∫—Å—Ç–∞
freeMp3Text.BackgroundTransparency = 1 -- –ü—Ä–æ–∑—Ä–∞—á–Ω—ã–π —Ñ–æ–Ω
freeMp3Text.Text = "FREE MP3! :3"
freeMp3Text.TextColor3 = Color3.new(1, 1, 0) -- –ñ–µ–ª—Ç—ã–π —Ü–≤–µ—Ç —Ç–µ–∫—Å—Ç–∞
freeMp3Text.Font = Enum.Font.SourceSansBold
freeMp3Text.TextScaled = true

-- –§—É–Ω–∫—Ü–∏—è –ø–µ—Ä–µ–∫–ª—é—á–µ–Ω–∏—è –º—É–∑—ã–∫–∏
local function toggleMusic()
    if music.Playing then
        music:Stop()
        playButton.Text = "START ‚äô‚Å†Ôπè‚Å†‚äô" -- –¢–µ–∫—Å—Ç –∫–Ω–æ–ø–∫–∏, –∫–æ–≥–¥–∞ –º—É–∑—ã–∫–∞ –æ—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∞
    else
        music:Play()
        playButton.Text = "‚ñ∂" -- –¢–µ–∫—Å—Ç –∫–Ω–æ–ø–∫–∏, –∫–æ–≥–¥–∞ –º—É–∑—ã–∫–∞ –∏–≥—Ä–∞–µ—Ç
    end
end


