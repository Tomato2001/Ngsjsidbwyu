    "rbxassetid://16258711993",
    "rbxassetid://16258713586",
    "rbxassetid://16258723227",
    "rbxassetid://16258724776"
}

-- –°–æ–∑–¥–∞–µ–º –¥–≤–∞ ImageLabel –Ω–∞ –≤—Ç–æ—Ä–æ–π —Å—Ç—Ä–∞–Ω–∏—Ü–µ
local gifImage1 = Instance.new("ImageLabel")
gifImage1.Parent = secondPage
gifImage1.Size = UDim2.new(0, 100, 0, 100)
gifImage1.Position = UDim2.new(1, -110, 1, -110)
gifImage1.BackgroundTransparency = 1
gifImage1.Visible = false -- –ù–∞—á–∏–Ω–∞–µ–º —Å –Ω–µ–≤–∏–¥–∏–º–æ–≥–æ —Å–æ—Å—Ç–æ—è–Ω–∏—è

local gifImage2 = gifImage1:Clone()
gifImage2.Parent = secondPage
gifImage2.Visible = true -- –í—Ç–æ—Ä–æ–π –≤–∏–¥–∏–º—ã–π —Å —Å–∞–º–æ–≥–æ –Ω–∞—á–∞–ª–∞

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Ü–∏–∫–ª–∏—á–µ—Å–∫–æ–≥–æ –∏–∑–º–µ–Ω–µ–Ω–∏—è –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏–π
local function animateGif()
    local currentIndex = 1
    local activeImage = gifImage1
    local inactiveImage = gifImage2

    while true do
        -- –ó–∞–≥—Ä—É–∂–∞–µ–º —Å–ª–µ–¥—É—é—â–µ–µ –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏–µ –≤ –Ω–µ–∞–∫—Ç–∏–≤–Ω—ã–π ImageLabel
        inactiveImage.Image = imageIds[currentIndex]

        -- –ü–æ–¥–æ–∂–¥–µ–º –Ω–µ–º–Ω–æ–≥–æ, –ø—Ä–µ–∂–¥–µ —á–µ–º –¥–µ–ª–∞—Ç—å –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏–µ –≤–∏–¥–∏–º—ã–º, —á—Ç–æ–±—ã —É–º–µ–Ω—å—à–∏—Ç—å –º–µ—Ä—Ü–∞–Ω–∏–µ
        -- –í–∞–∂–Ω–æ: —ç—Ç–æ –Ω–µ –≥–∞—Ä–∞–Ω—Ç–∏—Ä—É–µ—Ç –ø–æ–ª–Ω–æ–π –∑–∞–≥—Ä—É–∑–∫–∏ –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏—è, –Ω–æ –º–æ–∂–µ—Ç –ø–æ–º–æ—á—å –≤ –Ω–µ–∫–æ—Ç–æ—Ä—ã—Ö —Å–ª—É—á–∞—è—Ö
        wait(0.060) -- –í—ã –º–æ–∂–µ—Ç–µ –∞–¥–∞–ø—Ç–∏—Ä–æ–≤–∞—Ç—å –∑–∞–¥–µ—Ä–∂–∫—É, —á—Ç–æ–±—ã —É–≤–∏–¥–µ—Ç—å, —á—Ç–æ —Ä–∞–±–æ—Ç–∞–µ—Ç –ª—É—á—à–µ –¥–ª—è –≤–∞—à–µ–π –∞–Ω–∏–º–∞—Ü–∏–∏

        -- –î–µ–ª–∞–µ–º –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏–µ –≤–∏–¥–∏–º—ã–º —Ç–æ–ª—å–∫–æ –ø–æ—Å–ª–µ –∑–∞–¥–µ—Ä–∂–∫–∏
        inactiveImage.Visible = true
        activeImage.Visible = false

        -- –ü–µ—Ä–µ–∫–ª—é—á–∞–µ–º –∞–∫—Ç–∏–≤–Ω—ã–π –∏ –Ω–µ–∞–∫—Ç–∏–≤–Ω—ã–π ImageLabels
        activeImage, inactiveImage = inactiveImage, activeImage

        -- –ü–µ—Ä–µ—Ö–æ–¥ –∫ —Å–ª–µ–¥—É—é—â–µ–º—É –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏—é
        currentIndex = currentIndex + 1
        if currentIndex > #imageIds then
            currentIndex = 1
        end

        -- –û—Å—Ç–∞–≤–ª—è–µ–º –∑–∞–¥–µ—Ä–∂–∫—É –º–µ–∂–¥—É –∫–∞–¥—Ä–∞–º–∏
        wait(0.030) -- –ê–¥–∞–ø—Ç–∏—Ä—É–π—Ç–µ —Å–∫–æ—Ä–æ—Å—Ç—å –∞–Ω–∏–º–∞—Ü–∏–∏ –ø–æ –Ω–µ–æ–±—Ö–æ–¥–∏–º–æ—Å—Ç–∏
    end
end

-- –ó–∞–ø—É—Å–∫ –∞–Ω–∏–º–∞—Ü–∏–∏ –≤ –æ—Ç–¥–µ–ª—å–Ω–æ–º –ø–æ—Ç–æ–∫–µ
spawn(animateGif)

